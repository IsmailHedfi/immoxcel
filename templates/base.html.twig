        <script>
            function toggleMenu(menuIndex) {
            var menuSub = document.querySelectorAll('.menu-sub')[menuIndex];
            if (menuSub) {
                if (menuSub.style.display === 'none' || menuSub.style.display === '') {
                    menuSub.style.display = 'block';
                } else {
                menuSub.style.display = 'none';
            }
        }
    }
