{% extends 'base.html.twig' %}

{% block body %}


	<div class="main-content position-relative max-height-vh-100 h-100">
		<nav class="navbar navbar-main navbar-expand-lg px-0 mx-4 shadow-none border-radius-xl " id="navbarBlur" data-scroll="false">
			<div class="container-fluid py-1 px-3">
				<nav aria-label="breadcrumb">
					<ol class="breadcrumb bg-transparent mb-0 pb-0 pt-1 px-0 me-sm-6 me-5">
						<li class="breadcrumb-item text-sm">
							<a class="opacity-5 text-white" href="javascript:;">Pages</a>
						</li>
						<li class="breadcrumb-item text-sm text-white active" aria-current="page">Dashboard</li>
					</ol>
					<h6 class="font-weight-bolder text-white mb-0">Dashboard</h6>
				</nav>
				<div class="collapse navbar-collapse mt-sm-0 mt-2 me-md-0 me-sm-4" id="navbar">
					<div class="ms-md-auto pe-md-3 d-flex align-items-center">
						<div class="input-group">
							<span class="input-group-text text-body">
								<i class="fas fa-search" aria-hidden="true"></i>
							</span>
							<input type="text" class="form-control" placeholder="Type here..." onfocus="focused(this)" onfocusout="defocused(this)">

						</div>

					</div>
					<a href="javascript:;" class="nav-link text-white font-weight-bold px-0">
						<i class="fa fa-user me-sm-1"></i>
						<span class="d-sm-inline d-none">{{username}}</span>
					</a>
					<a href="{{path('go_to_logout')}}" class="btn btn-sm btn-danger mt-1">Logout</a>
				</li>
			</ul>
		</div>
	</div>
</body></nav><div class="container-fluid py-8">
<div class="row">
	<div class="col-md-0">
		{{ form_start(f,{'attr':{'novalidate': 'novalidate'}}) }}
		<div class="card">
			<div class="card-header pb-0">
				<div class="d-flex align-items-center">
					<p class="mb-0">Add Materials</p>

					{{ form_widget(f.Submit,{'label':'ADD','attr':{'class':'btn btn-primary btn-sm ms-auto'}}) }}
				</div>
			</div>
			<div class="card-body">
				<div class="row">
					<div class="col-md-6">
						<div class="form-group">
							<label for="example-text-input" class="form-control-label">Type Material</label>
							{{ form_widget(f.TypeMaterial,{'attr':{'class':'form-control'}}) }}
							{{ form_errors(f.TypeMaterial)}}

						</div>
					</div>
					<div class="col-md-6">
						<div class="form-group">
							<label for="example-text-input" class="form-control-label">Unit Price</label>
							{{ form_widget(f.UnitPrice,{'attr':{'class':'form-control'}}) }}
							{{ form_errors(f.UnitPrice)}}
						</div>
					</div>
					<div class="col-md-6">
						<div class="form-group">
							<label for="example-text-input" class="form-control-label">Quantity</label>
							{{ form_widget(f.Quantity,{'attr':{'class':'form-control'}}) }}
							{{ form_errors(f.Quantity)}}
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
	{{ form_end(f) }}
</div></div></div></div>{% endblock %}
