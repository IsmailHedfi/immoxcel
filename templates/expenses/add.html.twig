{% extends 'base.html.twig' %}
{% block title %}
	Add Transaction
{% endblock %}
{% block body %}
	<main
		class="main-content position-relative border-radius-lg">
		<!-- Navbar -->
		<nav
			class="navbar navbar-main navbar-expand-lg px-0 mx-4 shadow-none border-radius-xl" id="navbarBlur" data-scroll="false"><!-- Navbar content here -->
		</nav>
		<!-- End Navbar -->
		<div class="container-fluid py-4">
			<div class="row">
				<div class="col-8">
					<div class="card mb-10">
						<div
							class="card-header pb-0">
							<!-- Card header content here -->
							<div class="float-start">
								<h5 class="mt-3 mb-0">Add Expenses</h5>
							</div>
						</div>
						<div class="card-body">
							<div class="row">
								<div class="col-md-20">
									<div class="form-group">
										<div class="row">
											{{ form_start(f, {'attr': {'novalidate': 'novalidate'}}) }}
											<div class="col-md-12 mb-3">
												{{ form_row(f.Type, {'attr': {'class': 'form-control'}}) }}
												{{ form_errors(f.Type) }}

											</div>
											<div class="col-md-12 mb-3">
												{{ form_label(f.QuantityE,"Quantity") }}
												{{ form_widget(f.QuantityE, {'attr': {'class': 'form-control'}}) }}

											</div>


											<div class="col-md-12 mb-3">
												{{ form_row(f.coast, {'attr': {'class': 'form-control'}}) }}
											</div>

											<div class="col-md-12 mb-3">
												{{ form_row(f.Description, {'attr': {'class': 'form-control'}}) }}
											</div>
											{# <div class="col-md-12 mb-3">
												{{ form_label(f.product, "Products") }}
												{{ form_widget(f.product, {'attr': {'class': 'form-control'}}) }}
											</div> #}
											<div class="col-md-12 mb-3">
												{{ form_row(f.supplier, {'attr': {'class': 'form-control'}}) }}
											</div>
											{# <div class="col-md-12 mb-3">
												{{ form_row(f.project, {'attr': {'class': 'form-control'}}) }}
											</div> #}

											<div class="col-md-12 text-end">
												<button type="submit" class="btn btn-danger ">
													<svg xmlns="http://www.w3.org/2000/svg" width="22" height="22" fill="currentColor" class="bi bi-download" viewbox="0 0 16 16">
														<path d="M.5 9.9a.5.5 0 0 1 .5.5v2.5a1 1 0 0 0 1 1h12a1 1 0 0 0 1-1v-2.5a.5.5 0 0 1 1 0v2.5a2 2 0 0 1-2 2H2a2 2 0 0 1-2-2v-2.5a.5.5 0 0 1 .5-.5"/>
														<path d="M7.646 11.854a.5.5 0 0 0 .708 0l3-3a.5.5 0 0 0-.708-.708L8.5 10.293V1.5a.5.5 0 0 0-1 0v8.793L5.354 8.146a.5.5 0 1 0-.708.708z"/>
													</svg>
													Save</button>
											</div>
											{{ form_end(f) }}
										</div>
									</div>
								</div>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
	</main>
{% endblock %}
